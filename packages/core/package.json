{
	"name": "@wpkernel/core",
	"version": "0.12.3-beta.0",
	"description": "Rails-like framework for building modern WordPress products",
	"license": "EUPL-1.2",
	"type": "module",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/wpkernel/wpkernel.git",
		"directory": "packages/core"
	},
	"homepage": "https://github.com/wpkernel/wpkernel#readme",
	"bugs": {
		"url": "https://github.com/wpkernel/wpkernel/issues"
	},
	"keywords": [
		"wordpress",
		"framework",
		"blocks",
		"gutenberg",
		"interactivity",
		"actions",
		"resources",
		"rest-api"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./http": {
			"types": "./dist/http/index.d.ts",
			"import": "./dist/http.js",
			"default": "./dist/http.js"
		},
		"./resource": {
			"types": "./dist/resource/index.d.ts",
			"import": "./dist/resource.js",
			"default": "./dist/resource.js"
		},
		"./error": {
			"types": "./dist/error/index.d.ts",
			"import": "./dist/error.js",
			"default": "./dist/error.js"
		},
		"./actions": {
			"types": "./dist/actions/index.d.ts",
			"import": "./dist/actions.js",
			"default": "./dist/actions.js"
		},
		"./data": {
			"types": "./dist/data/index.d.ts",
			"import": "./dist/data.js",
			"default": "./dist/data.js"
		},
		"./events": {
			"types": "./dist/events/index.d.ts",
			"import": "./dist/events.js",
			"default": "./dist/events.js"
		},
		"./capability": {
			"types": "./dist/capability/index.d.ts",
			"import": "./dist/capability.js",
			"default": "./dist/capability.js"
		},
		"./pipeline": {
			"types": "./dist/pipeline/index.d.ts",
			"import": "./dist/pipeline.js",
			"default": "./dist/pipeline.js"
		},
		"./reporter": {
			"types": "./dist/reporter/index.d.ts",
			"import": "./dist/reporter.js",
			"default": "./dist/reporter.js"
		},
		"./interactivity": {
			"types": "./dist/interactivity/index.d.ts",
			"import": "./dist/interactivity.js",
			"default": "./dist/interactivity.js"
		},
		"./namespace": {
			"types": "./dist/namespace/index.d.ts",
			"import": "./dist/namespace.js",
			"default": "./dist/namespace.js"
		},
		"./namespace/constants": {
			"types": "./dist/namespace/constants.d.ts",
			"import": "./dist/namespace/constants.js",
			"default": "./dist/namespace/constants.js"
		},
		"./contracts": {
			"types": "./dist/contracts/index.d.ts",
			"import": "./dist/contracts.js",
			"default": "./dist/contracts.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build && tsc --noEmit",
		"clean": "rm -rf dist *.tsbuildinfo",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write 'src/**/*.{js,ts,tsx,json,md}'",
		"format:check": "prettier --check 'src/**/*.{js,ts,tsx,json,md}'",
		"typecheck": "tsc --noEmit",
		"typecheck:tests": "wpk-run-typecheck-tests",
		"test": "wpk-run-jest",
		"test:watch": "wpk-run-jest --watch",
		"test:coverage": "wpk-run-jest coverage",
		"test:integration": "wpk-run-jest integration"
	},
	"devDependencies": {
		"@wpkernel/scripts": "workspace:*"
	},
	"peerDependencies": {
		"@wordpress/api-fetch": ">=7.32.0",
		"@wordpress/data": ">=10.32.0",
		"@wordpress/element": ">=6.32.0",
		"@wordpress/hooks": ">=4.32.0",
		"@wordpress/interactivity": ">=6.34.0",
		"loglayer": ">=6.7.2",
		"@loglayer/shared": ">=2.4.0",
		"@loglayer/transport": ">=2.3.0",
		"@loglayer/transport-simple-pretty-terminal": ">=2.2.9"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"tag": "beta"
	},
	"dependencies": {
		"@wpkernel/pipeline": "workspace:*"
	}
}
