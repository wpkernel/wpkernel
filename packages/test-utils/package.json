{
	"name": "@wpkernel/test-utils",
	"version": "0.12.6-beta.3",
	"description": "Shared testing utilities for WPKernel packages",
	"license": "EUPL-1.2",
	"type": "module",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/wpkernel/wpkernel.git",
		"directory": "packages/test-utils"
	},
	"homepage": "https://github.com/wpkernel/wpkernel#readme",
	"bugs": {
		"url": "https://github.com/wpkernel/wpkernel/issues"
	},
	"keywords": [
		"wordpress",
		"testing",
		"harness",
		"cli",
		"integration"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./wp": {
			"types": "./dist/wp/index.d.ts",
			"import": "./dist/wp/index.js",
			"default": "./dist/wp/index.js"
		},
		"./integration": {
			"types": "./dist/integration/index.d.ts",
			"import": "./dist/integration/index.js",
			"default": "./dist/integration/index.js"
		},
		"./integration/workspace": {
			"types": "./dist/integration/workspace.d.ts",
			"import": "./dist/integration/workspace.js",
			"default": "./dist/integration/workspace.js"
		},
		"./core": {
			"types": "./dist/core/index.d.ts",
			"import": "./dist/core/index.js",
			"default": "./dist/core/index.js"
		},
		"./core/wp-harness": {
			"types": "./dist/core/wp-harness.d.ts",
			"import": "./dist/core/wp-harness.js",
			"default": "./dist/core/wp-harness.js"
		},
		"./core/action-runtime": {
			"types": "./dist/core/action-runtime.d.ts",
			"import": "./dist/core/action-runtime.js",
			"default": "./dist/core/action-runtime.js"
		},
		"./ui": {
			"types": "./dist/ui/index.d.ts",
			"import": "./dist/ui/index.js",
			"default": "./dist/ui/index.js"
		},
		"./ui/kernel-ui-harness": {
			"types": "./dist/ui/kernel-ui-harness.d.ts",
			"import": "./dist/ui/kernel-ui-harness.js",
			"default": "./dist/ui/kernel-ui-harness.js"
		},
		"./layout.test-support": {
			"types": "./dist/layout.test-support.d.ts",
			"import": "./dist/layout.test-support.js",
			"default": "./dist/layout.test-support.js"
		},
		"./layout-manifest.test-support": {
			"types": "./dist/layout-manifest.test-support.d.ts",
			"import": "./dist/layout-manifest.test-support.js",
			"default": "./dist/layout-manifest.test-support.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"prebuild": "node ../../scripts/workspace-graph.prebuild.mjs @wpkernel/test-utils",
		"dev": "tsc --project tsconfig.json --watch",
		"build": "vite build && tsc --project tsconfig.json",
		"clean": "rm -rf dist *.tsbuildinfo",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write 'src/**/*.{ts,tsx}'",
		"format:check": "prettier --check 'src/**/*.{ts,tsx}'",
		"typecheck": "tsc --project tsconfig.json --noEmit --rootDir ../..",
		"typecheck:tests": "wpk-run-typecheck-tests",
		"test": "wpk-run-jest",
		"test:watch": "wpk-run-jest --watch",
		"test:coverage": "wpk-run-jest coverage",
		"test:integration": "wpk-run-jest integration"
	},
	"devDependencies": {
		"@wpkernel/scripts": "workspace:*"
	},
	"peerDependencies": {
		"@wpkernel/core": "workspace:*",
		"@wpkernel/pipeline": "workspace:*",
		"@wpkernel/ui": "workspace:*",
		"react": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"tag": "beta"
	}
}
