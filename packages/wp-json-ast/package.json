{
	"name": "@wpkernel/wp-json-ast",
	"version": "0.12.4-beta.0",
	"description": "WordPress-specific extensions for the @wpkernel/php-json-ast toolkit",
	"license": "EUPL-1.2",
	"type": "module",
	"sideEffects": false,
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "vite build && tsc --noEmit",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"test": "wpk-run-jest",
		"test:coverage": "wpk-run-jest coverage",
		"test:watch": "wpk-run-jest --watch",
		"test:integration": "wpk-run-jest integration",
		"typecheck": "tsc --noEmit",
		"typecheck:tests": "wpk-run-typecheck-tests"
	},
	"devDependencies": {
		"@wpkernel/scripts": "workspace:*"
	},
	"peerDependencies": {
		"@wpkernel/core": "workspace:*",
		"@wpkernel/php-json-ast": "workspace:*"
	},
	"dependencies": {
		"@wpkernel/core": "workspace:*",
		"@wpkernel/php-json-ast": "workspace:*"
	}
}
