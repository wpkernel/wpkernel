{
	"name": "@wpkernel/ui",
	"version": "0.12.5-beta.0",
	"description": "UI component library for WPKernel",
	"license": "EUPL-1.2",
	"type": "module",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/wpkernel/wpkernel.git",
		"directory": "packages/ui"
	},
	"homepage": "https://github.com/wpkernel/wpkernel#readme",
	"bugs": {
		"url": "https://github.com/wpkernel/wpkernel/issues"
	},
	"keywords": [
		"wordpress",
		"components",
		"ui",
		"gutenberg",
		"blocks",
		"react"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./hooks/useAction": {
			"types": "./dist/hooks/useAction.d.ts",
			"import": "./dist/hooks/useAction.js",
			"default": "./dist/hooks/useAction.js"
		},
		"./hooks/useTaxonomyOptions": {
			"types": "./dist/hooks/useTaxonomyOptions.d.ts",
			"import": "./dist/hooks/useTaxonomyOptions.js",
			"default": "./dist/hooks/useTaxonomyOptions.js"
		},
		"./hooks/useCapability": {
			"types": "./dist/hooks/useCapability.d.ts",
			"import": "./dist/hooks/useCapability.js",
			"default": "./dist/hooks/useCapability.js"
		},
		"./hooks/resource-hooks": {
			"types": "./dist/hooks/resource-hooks.d.ts",
			"import": "./dist/hooks/resource-hooks.js",
			"default": "./dist/hooks/resource-hooks.js"
		},
		"./hooks/usePrefetcher": {
			"types": "./dist/hooks/usePrefetcher.d.ts",
			"import": "./dist/hooks/usePrefetcher.js",
			"default": "./dist/hooks/usePrefetcher.js"
		},
		"./hooks/useHoverPrefetch": {
			"types": "./dist/hooks/useHoverPrefetch.d.ts",
			"import": "./dist/hooks/useHoverPrefetch.js",
			"default": "./dist/hooks/useHoverPrefetch.js"
		},
		"./hooks/useVisiblePrefetch": {
			"types": "./dist/hooks/useVisiblePrefetch.d.ts",
			"import": "./dist/hooks/useVisiblePrefetch.js",
			"default": "./dist/hooks/useVisiblePrefetch.js"
		},
		"./hooks/useNextPagePrefetch": {
			"types": "./dist/hooks/useNextPagePrefetch.d.ts",
			"import": "./dist/hooks/useNextPagePrefetch.js",
			"default": "./dist/hooks/useNextPagePrefetch.js"
		},
		"./dataviews/types": {
			"types": "./dist/dataviews/types.d.ts",
			"default": "./dist/dataviews/types.d.ts"
		},
		"./dataviews": {
			"types": "./dist/dataviews/index.d.ts",
			"import": "./dist/dataviews.js",
			"default": "./dist/dataviews.js"
		},
		"./dataviews/data-form-controller": {
			"types": "./dist/dataviews/data-form-controller.d.ts",
			"import": "./dist/dataviews/data-form-controller.js",
			"default": "./dist/dataviews/data-form-controller.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build && tsc --noEmit",
		"clean": "rm -rf dist *.tsbuildinfo",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write 'src/**/*.{js,ts,tsx,json,md}'",
		"format:check": "prettier --check 'src/**/*.{js,ts,tsx,json,md}'",
		"typecheck": "tsc --noEmit",
		"typecheck:tests": "wpk-run-typecheck-tests",
		"test": "wpk-run-jest",
		"test:watch": "wpk-run-jest --watch",
		"test:coverage": "wpk-run-jest coverage",
		"test:integration": "wpk-run-jest integration"
	},
	"devDependencies": {
		"@wpkernel/scripts": "workspace:*"
	},
	"peerDependencies": {
		"@wpkernel/core": "workspace:*",
		"@testing-library/react": ">=16.3.0",
		"@wordpress/dataviews": ">=9.1.0",
		"@wordpress/components": ">=30.5.0",
		"@wordpress/data": ">=10.32.0",
		"@wordpress/element": ">=6.32.0",
		"react": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"tag": "beta"
	}
}
